     list xemp
        table prmst
        order xemp
        select "(xemp like '%"+zvalue+"%' or xname like '%"+zvalue+"%')"
        navigators alpha
        rows 100
        objects xemp attrib(link "abc" embed onclick="return updateParent(this:'')"), ~
                xname,xdesig

        field xemp
          caption Employee Code
        end field

        field xname
          caption Employee Name
        end field
     end list


