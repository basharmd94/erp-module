format prdum
  file prdum.txt
  table pratdummy
  fdelimiter :
  rdelimiter /r/n
  skip 0
  columns xname,xemp,xref,xdesc,xstatusjv,xpflag

  event before

    set pratdummy.zid = #id
    set pratdummy.xpflag=""
    set pratdummy.ztime="7/1/2007 12:43:38 PM"
    set pratdummy.zutime="7/1/2007 12:43:38 PM"
  end event

end format
